<?xml version="1.0" encoding="UTF-8"?>
<data>
	
	<events>
		<event id="onEnterScrollView">			
			<!-- <media id="animTimeline" action="play" /> -->
			<media id="osc" action="play" />
			<media id="wheel" action="play" />
			<media id="wheel2" action="play" />
		</event>
		
		<event id="onExitScrollView">		
			<!-- <media id="animTimeline" action="reverse" /> -->
			<media id="osc" action="stop" />
			<media id="wheel" action="stop" />
			<media id="wheel2" action="stop" />
		</event>
	</events>
	<!--
	<timeline id="animTimeline" autoplay="false" time="1">
		<tween element="leftDoor" time="0.5" delay="1"><![CDATA[ x:-98%, ease:Linear.easeNone  ]]></tween>
		<tween element="rightDoor" time="0.5"  delay="1"><![CDATA[ x:98%, ease:Linear.easeNone  ]]></tween>
	</timeline>
	-->
	<svganim id="osc" src="lib/animations/menu_science/mdl851002_menu_3_1.json" width="200" height="200" loop="true" autoplay="false" disableTM="true" fps="40"/>
	<svganim id="wheel" src="lib/animations/menu_science/mdl851002_menu_3_2.json" x="-60" y="6" width="100%" height="200" loop="true" autoplay="false" disableTM="true"/>
	<svganim id="wheel2" src="lib/animations/menu_science/mdl851002_menu_3_2.json" x="0" y="6" width="100%" height="200" loop="true" autoplay="false" disableTM="true"/>
</data>
